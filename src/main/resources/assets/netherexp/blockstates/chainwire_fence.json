      },
      "when": {
        "east": "true",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/cap_alt"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "true",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/cap_alt",
        "y": 90
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/side"
      },
      "when": {
