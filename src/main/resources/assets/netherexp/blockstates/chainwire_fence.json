{
  "multipart": [
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/post_ends"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/post"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/cap"
      },
      "when": {
        "east": "false",
        "north": "true",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/cap",
