      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "netherexp:block/chainwire_fence/side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    }
  ]
}